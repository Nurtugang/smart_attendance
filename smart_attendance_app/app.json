{
  "expo": {
    "name": "Farabi Smart Attendance",
    "slug": "smart-attendance-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon-sas.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/splash-icon-sas.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon-sas.png",
        "backgroundColor": "#ffffff"
      },
      "edgeToEdgeEnabled": true
    },
    "web": {
      "favicon": "./assets/favicon-sas.png"
    },
    "plugins": [
      "expo-secure-store"
    ]
  }
}
